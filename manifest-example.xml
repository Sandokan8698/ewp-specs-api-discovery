    <dev-notes>It's not a real manifest. Just an example.</dev-notes>
    <apis-implemented>

        <!-- The entry for the Discovery Manifest API is defined in the manifest's
        own XML namespace. -->

        <discovery version="3.0.0">
            <ewp:dev-email>admin2@example.com</ewp:dev-email>
            <ewp:dev-email>admin3@example.com</ewp:dev-email>
            <url>https://example.com/manifest.xml</url>
        </discovery>

        <!-- All other manifest entries are defined in their own XML namespaces.
        We need to use proper `xmlns` in order for them to be matched properly. -->

        <echo
            xmlns="https://github.com/erasmus-without-paper/ewp-specs-api-echo/blob/master/manifest-entry.xsd"
            ewp:version="0.0.0"
        >
            <ewp:dev-email>admin4@example.com</ewp:dev-email>
            <ewp:dev-notes>Notes may be supplied in API-context too.</ewp:dev-notes>
            <url>https://example.com/ewp/echo</url>
        </echo>

        <departments
            xmlns="https://github.com/erasmus-without-paper/ewp-specs-api-departments/blob/master/manifest-entry.xsd"
            ewp:version="0.0.0"
        >
            <url>https://example.com/ewp/departments</url>
        </departments>

